      "configuration" : {
        "scriptLang" : "JS",
        "jsScript" : "\r\n//Last known working offline detection\r\n\r\nvar now = Date.now();\r\n\r\nvar last = Number(metadata.ss_latestHbTs || 0);\r\nif (!last) {\r\n  return { msg: {}, metadata: metadata, msgType: \"POST_ATTRIBUTES_REQUEST\" };\r\n}\r\n\r\nvar STALE_MS = 90000;    // 2 min = 120000\r\nvar OFFLINE_MS = 150000;  // 4 min = 240000\r\n\r\nvar age = now - last;\r\n\r\nvar state = \"ONLINE\";\r\nif (age >= OFFLINE_MS) {\r\n    state = \"OFFLINE\";\r\n} \r\nelse if (age >= STALE_MS) state = \"STALE\";\r\n\r\nvar prev = String(metadata.ss_psuState || \"\");\r\nif (prev === state) {\r\n  return { msg: {}, metadata: metadata, msgType: \"POST_ATTRIBUTES_REQUEST\" };\r\n}\r\n\r\nvar out = { psuState: state };\r\n\r\n// entering OFFLINE: stamp once\r\nif (state === \"OFFLINE\") {\r\n  out.offlineSinceTs = metadata.ss_latestHbTs;\r\n}\r\n\r\n// leaving OFFLINE: clear\r\nif (prev === \"OFFLINE\" && state !== \"OFFLINE\") {\r\n  out.offlineSinceTs = 0;\r\n}\r\n\r\nreturn {\r\n  msg: out,\r\n  metadata: metadata,\r\n  msgType: \"POST_ATTRIBUTES_REQUEST\"\r\n};\r\n\r\n",
